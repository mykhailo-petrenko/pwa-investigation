{
  "name": "pwa-investigation",
  "version": "1.0.0",
  "description": "PWA Investigation",
  "main": "src/app.js",
  "scripts": {
    "start": "webpack-dev-server --open --env.development --config webpack.config.js",
    "lint": "node_modules/.bin/esw webpack.config.js src .build",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --compilers js:babel-core/register .build/testSetup.js \"test/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch",
    "remove:target": "rimraf ./target",
    "clean": "npm run remove:target && mkdir target",
    "prebuild": "npm run clean && npm run lint && npm run test",
    "build": "webpack --env.production --config webpack.config.js"
  },
  "author": "Mykhailo Petrenko",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^4.0.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.1.1",
    "eslint-plugin-chai-friendly": "^0.3.6",
    "eslint-plugin-import": "^2.6.1",
    "eslint-watch": "^3.1.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.29.0",
    "mocha": "^3.4.2",
    "ngtemplate-loader": "^2.0.1",
    "node-sass": "^4.5.3",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "@uirouter/angularjs": "^1.0.5",
    "angular": "^1.6.4",
    "angular-animate": "^1.6.6",
    "angular-aria": "^1.6.6",
    "angular-material": "^1.1.5",
    "angular-messages": "^1.6.6"
  }
}
